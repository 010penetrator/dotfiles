let scrollstep = 141
set cncpcompletion
set noautofocus
unmap E R S D a e d u
map ,  gT
map .  gt
map J  gt
map K  gT
map gb gT
map at :tabnew<CR>
map af L
map ab H
map aa i
map u lastClosedTab
let fullpagescrollpercent = 82
map d  scrollFullPageDown
map e  scrollFullPageUp
map <Space> scrollFullPageDown
map <S-Space> scrollFullPageUp
map <BS> scrollFullPageUp
map ax 99gxt
map aq :qall<CR>
map ac :open chrome-extension://ihlenndgcmojhcghmfjfneahoeklbjjh/pages/blank.html<CR>
map ad :tabdetach<CR>
map as :tabattach<space>
map ai fullImageHint
map ae createEditHint
map A  :bookmarks<space>
map b  e
map zf toggleImageZoom
map gh :open opennet.ru<CR>
map [x gxT
map ]x gxt

